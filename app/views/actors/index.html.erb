<h2 class='text-center'>Our Current Actors</h2>
<hr>
<br>
<div class="row text-center">
  <% @actors.each do |actor| %>
    <div class="col-md-4">
      <%= actor.thumbnail %>
      <img src="/<%= actor.thumbnail %>">
      <p><%= actor.name %></p>
      <p><%= actor.year_of_birth %></p>
    </div>
<% end %>
</div>
